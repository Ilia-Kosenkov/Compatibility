<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>IndexRange</id>
    <version>2.0.3</version>
    <developmentDependency>true</developmentDependency>
    <authors>Ilia Kosenkov</authors>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/Ilia-Kosenkov/Compatibility</projectUrl>
    <repository type="git" url="https://github.com/Ilia-Kosenkov/Compatibility"/>
    <description>A substitution for the Index/Range types.</description>
    <contentFiles>
      <files include="**/*.cs.pp"/>
    </contentFiles>
  </metadata>
  <files>
    
    <!-- Inspired by https://github.com/manuelroemer/Nullable/blob/master/src/Nullable.nuspec -->
    
    <file src="Index.cs.pp" exclude="**/obj/**/*.cs.pp" target="content/netstandard2.0/IndexRange/Index.cs.pp"/>
    <file src="Index.cs.pp" exclude="**/obj/**/*.cs.pp" target="content/net40/IndexRange/Index.cs.pp"/>
      
    <file src="Range.cs.pp" exclude="**/obj/**/*.cs.pp" target="content/netstandard2.0/IndexRange/Range.cs.pp"/>
    <file src="Range.cs.pp" exclude="**/obj/**/*.cs.pp" target="content/net40/IndexRange/Range.cs.pp"/>

    <file src="IndexRangeExtensions.cs.pp" exclude="**/obj/**/*.cs.pp" target="content/netstandard2.0/IndexRange/IndexRangeExtensions.cs.pp"/>
    <file src="IndexRangeExtensions.cs.pp" exclude="**/obj/**/*.cs.pp" target="content/net40/IndexRange/IndexRangeExtensions.cs.pp"/>


    
    <file src="Index.cs.pp" exclude="**/obj/**/*.cs.pp" target="contentFiles/cs/netstandard2.0/IndexRange/Index.cs.pp"/>
    <file src="Index.cs.pp" exclude="**/obj/**/*.cs.pp" target="contentFiles/cs/net40/IndexRange/Index.cs.pp"/>
      
    <file src="Range.cs.pp" exclude="**/obj/**/*.cs.pp" target="contentFiles/cs/netstandard2.0/IndexRange/Range.cs.pp"/>
    <file src="Range.cs.pp" exclude="**/obj/**/*.cs.pp" target="contentFiles/cs/net40/IndexRange/Range.cs.pp"/>

    <file src="IndexRangeExtensions.cs.pp" exclude="**/obj/**/*.cs.pp" target="contentFiles/cs/netstandard2.0/IndexRange/IndexRangeExtensions.cs.pp"/>
    <file src="IndexRangeExtensions.cs.pp" exclude="**/obj/**/*.cs.pp" target="contentFiles/cs/net40/IndexRange/IndexRangeExtensions.cs.pp"/>
   
    <!-- EXCLUDE the attributes in these target frameworks: -->
    <file src=".nuget/_._" target="contentFiles/cs/netstandard2.1"/>
    <file src=".nuget/_._" target="content/netstandard2.1"/>

  </files>
</package>

   
